{
  "name": "esc-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "check-types": "tsc --noEmit",
    "heroku-postbuild": "npm run build",
    "format": "prettier --write src tests",
    "lint": "eslint src --ext ts",
    "lint-fix": "npm run lint -- --fix",
    "start": "nodemon --watch \"src/**/*.ts\" -e ts --exec \"ts-node src/bot.ts | pino-pretty\"",
    "test": "npm run test-only && npm run lint && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ES-Community/bot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ES-Community/bot/issues"
  },
  "homepage": "https://github.com/ES-Community/bot#readme",
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "cron": "^1.8.2",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.2.2",
    "got": "^11.8.2",
    "make-promises-safe": "^5.1.0",
    "pino": "^6.11.1"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/emoji-regex": "^8.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@types/pino": "^6.3.6",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "14.x"
  }
}
